    <select id="roomList" parameterType="Room" resultType="Room">
        select * from bbh_room_room
        <where>
            <if test="room!=null">
                and room=#{room}
            </if>
            <if test="size!=null and size==0">
                and size=2
            </if>
            <if test="size!=null and size==1">
                and size=4
            </if>
            <if test="size!=null and size==2">
                and size=8
            </if>
            <if test="square!=null and square==0">
                and square <![CDATA[<=]]>60
            </if>
            <if test="square!=null and square==1">
                and square&gt;=60 and square <![CDATA[<=]]>100
            </if>
            <if test="square!=null and square==2">
                and square&gt;=100
            </if>
            <if test="face!=null and face==0">
                and square='东'
            </if>
            <if test="square!=null and square==1">
                and square='西'
            </if>
        </where>
    </select>
    <select id="selectRoomById" parameterType="java.lang.Integer" resultType="Room">
        select bbh_room_room.room, bbh_room_room.size
        ,bbh_room_room.square,bbh_room_room.face,bbh_user_user.realName from bbh_room_room INNER JOIN bbh_user_user on
        bbh_room_room.id=bbh_user_user.roomId WHERE bbh_room_room.id=#{id}
      </select>

